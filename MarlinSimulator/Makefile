	rm -f $(OBJ_RELEASE) $(OUT_RELEASE)
	rm -rf .bin/Release
	rm -rf $(OBJDIR_RELEASE)/arduino_sim
	rm -rf $(OBJDIR_RELEASE)/__/Marlin
	rm -rf $(OBJDIR_RELEASE)/component
	rm -rf $(OBJDIR_RELEASE)
	rm -rf $(OBJDIR_RELEASE)/avr_sim/avr

